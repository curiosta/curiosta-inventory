{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "noEmit": true,
    "allowJs": true,
    "checkJs": true,
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "baseUrl": ".",
    "paths": {
      "@assets/*": [
        "src/assets/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@/*": [
        "src/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@api/*": [
        "src/api/*"
      ],
      "@store/*": [
        "src/store/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@Toggle/*": [
        "src/components/Toggle/*"
      ],
      "@regions/*": [
        "src/api/regions/*"
      ],
      "@region/*": [
        "src/api/region/*"
      ]
    }
  },
  "include": [
    "node_modules/vite/client.d.ts",
    "**/*"
  ]
}